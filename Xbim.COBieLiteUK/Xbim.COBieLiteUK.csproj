<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net47</TargetFrameworks>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>5.0.0.0</Version>
    <AssemblyVersion>5.0.0.0</AssemblyVersion>
    <FileVersion>5.0.0.0</FileVersion>
    <Title>Xbim.CobieLiteUK</Title>
    <Copyright>Xbim Ltd.</Copyright>
    <Description>
      UK COBieLite schema for the XBIM Exchange toolkit
    </Description>
    <Authors>xBIM Team</Authors>
    <Company>Xbim Ltd.</Company>
    <PackageLicenseUrl>https://raw.githubusercontent.com/xBimTeam/XbimEssentials/master/LICENCE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/xBimTeam/XbimExchange</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xBimTeam/XbimExchanges/tree/develop</RepositoryUrl>
    <PackageTags>BIM, IFC, IfcXml, IfcZip, Ifc4, COBie, COBieLite</PackageTags>
    <PackageReleaseNotes>v5.0 support the latest Xbim.Essentials 5.0</PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/2284875?v=3&amp;amp;s=240</PackageIconUrl>
    <AssemblyName>Xbim.CobieLiteUK</AssemblyName>
    <RootNamespace>Xbim.CobieLiteUk</RootNamespace>
    <PackageId>Xbim.COBieLiteUK</PackageId>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\XbimOpenSourceKeyFile.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="FilterHelper\COBieArchitecturalFilters.config" />
    <None Remove="FilterHelper\COBieDefaultFilters.config" />
    <None Remove="FilterHelper\COBieElectricalFilters.config" />
    <None Remove="FilterHelper\COBieFireProtectionFilters.config" />
    <None Remove="FilterHelper\COBieMechanicalFilters.config" />
    <None Remove="FilterHelper\COBiePlumbingFilters.config" />
    <None Remove="Templates\UK2012.xls" />
    <None Remove="Templates\UK2012.xlsx" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="FilterHelper\COBieArchitecturalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieDefaultFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieElectricalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieFireProtectionFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBieMechanicalFilters.config" />
    <EmbeddedResource Include="FilterHelper\COBiePlumbingFilters.config" />
    <EmbeddedResource Include="Templates\UK2012.xls" />
    <EmbeddedResource Include="Templates\UK2012.xlsx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="Newtonsoft.Json.Bson" Version="1.0.1" />
    <PackageReference Include="NPOI" Version="2.4.0" />
    <PackageReference Include="Xbim.Essentials" Version="5.0.204" />
  </ItemGroup>

</Project>
